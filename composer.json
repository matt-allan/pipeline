{
    "name": "yuloh/pipeline",
    "type": "library",
    "description": "A simple functional pipeline",
    "keywords": [
        "yuloh",
        "pipeline",
        "functional"
    ],
    "homepage": "https://github.com/yuloh/pipeline",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Allan",
            "email": "matthew.james.allan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Yuloh\\Pipeline\\": "src"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Yuloh\\Pipeline\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs --standard=psr2 src/"
    }
}
